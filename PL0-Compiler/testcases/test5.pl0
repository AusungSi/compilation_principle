program LexerTest;

const
  maxVal := 100,     
  minVal := 0,
  factor := 2;

var 
  x, y, z, i, temp;

procedure gcd(a, b);
var t;
begin
  while b <> 0 do
  begin
    t := b;
    b := a - (a / b) * b;
    a := t
  end;
  z := a
end;

procedure checkOdd(num);
begin
  if odd num then
    write(1)
  else
    write(0)
end;

begin
  read(x, y);

  if x < y then
  begin
    temp := x;
    x := y;
    y := temp
  end;

  if x >= maxVal then
    x := maxVal;

  if y <= minVal then
    y := minVal + 1;

  call gcd(x, y);

  i := (x + y) * factor - 10 / 2;

  if i > 50 then
    call checkOdd(i);

  write(z, i)
end